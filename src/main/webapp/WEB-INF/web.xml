<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <display-name>CrunchifyJSPServletExample</display-name>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Servlet_Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Menu</servlet-name>
        <url-pattern>/Servlet_Menu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HSSFCreate</servlet-name>
        <url-pattern>/HSSFCreate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BarServlet</servlet-name>
        <url-pattern>/BarServlet</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>CrearUser.html</welcome-file>
        <welcome-file>CrearUser.htm</welcome-file>
        <welcome-file>CrearUser.jsp</welcome-file>
        <welcome-file>default.html</welcome-file>
        <welcome-file>default.htm</welcome-file>
        <welcome-file>default.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.crunchify.jsp.servlet.Servlet_Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Menu</servlet-name>
        <servlet-class>com.crunchify.jsp.servlet.Servlet_Menu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HSSFCreate</servlet-name>
        <servlet-class>com.crunchify.jsp.servlet.HSSFCreate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BarServlet</servlet-name>
        <servlet-class>com.crunchify.jsp.servlet.BarServlet</servlet-class>
    </servlet>
</web-app>
